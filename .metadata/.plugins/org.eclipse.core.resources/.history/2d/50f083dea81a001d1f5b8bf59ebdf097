
public class Main {

	public static void main(String[] args) {
		
		// && and
		// || or 
		// ! not
		
		int temp = 35;
		
		if(temp > 30) {
			System.out.println("It is hot outside");
		} else if(temp <=30 && temp >=20) {
			System.out.println("It is warm outside");
		} else {
			System.out.println("It is cold outside");
		}
		
	}

}
