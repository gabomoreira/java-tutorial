
public class Main {

	public static void main(String[] args) {
		
		// polymorphism = 	greek word for poly-"many", morph-"form"
		//					The ability of an object to identify as more than one type
	
		Car car = new Car();
		Boat boat = new Boat();
		Bicycle bicycle = new Bicycle();
		
		Vehicle[] racers = {car, boat, bicycle};
		
		car.go();
	}
}
