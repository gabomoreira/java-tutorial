
public class Main {

	public static void main(String[] args) {
		
		// method chaining = 	a common syntax for invoking multiple method calls in OOP
		//						condense code into less lines
	
		
		String name = "gabo";
		
		name = name.concat(" code").toUpperCase().trim();
		
		System.out.println(name);
	}

}
